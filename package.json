{
  "name": "fractal-viewer",
  "version": "1.0.0",
  "description": "a web app for exploring the Mandelbrot set",
  "repository": "https://github.com/austinrsands/fractal-viewer",
  "private": true,
  "homepage": "https://austinrsands.github.io/fractal-viewer/",
  "scripts": {
    "start": "parcel src/markup/index.html --open",
